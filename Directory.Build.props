    <PackageProjectUrl>$(RepositoryUrl)</PackageProjectUrl>

    <ImportNetSdkFromRepoToolset>false</ImportNetSdkFromRepoToolset>
  </PropertyGroup>

  <Import Project="Sdk.props" Sdk="RoslynTools.RepoToolset" />
</Project>
